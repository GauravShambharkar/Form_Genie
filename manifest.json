{
  "manifest_version": 3,
  "name": "Form Genie",
  "version": "1.0",
  "description": "Automatically fills forms after reload using saved data",
  "permissions": ["storage", "scripting", "activeTab"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"],
      "run_at": "document_idle"
    }
  ]
}
